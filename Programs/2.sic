
int main(){
    int n=24; 

    if(n==1){
        print("no\n");
        return 0;
    }
    bool prime; 
    prime = true;
    for (int i = 2; i < n; i += 1){
        if (n % i == 0){
            prime = false;
        }
    }
    if (prime) {
        print('y');
    }
    else {
        print('n');
    }
}