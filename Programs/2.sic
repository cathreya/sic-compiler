int main(){
    int n; 
    read(n);
    if(n==1){
        print("no\n");
        return 0;
    }
    bool prime; 
    prime = true;
    for (int i = 2; i < n; i += 1){
        if (n % i == 0){
            prime = false;
        }
    }
    if (prime) {
        print("yes\n");
    }
    else {
        print("no\n");
    }
}