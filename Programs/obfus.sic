import "io.sic";import "stdlib.sic";char[1000] author = "Athreya C, 2018121002";int readInt(){    char[20] num;    int i; i = 0;    char c;    while (true) {        c = read();        if (c == ' ') { break; }        num[i] = c;        i += 1;    }    return toInt(num);}void intToStr(int k, char[1000] num){    bool nonneg; nonneg = k >= 0;     char[20] revnum;    if (!nonneg) {        k *= -1;    }    int i; i = 0;    int z; z = '0';    while (k > 0){        revnum[i] = k % 10 + z;        i += 1;    }    int j; j = 0;    if (!nonneg){        num[j] = '-';        j += 1;    }    while (i >= 0){        i -= 1;        num[j] = revnum[i];        j += 1;    }    num[j] = '\0';}int isPrime(int n){    for(int i=2;i*i<n;i+=1){        if(n%i==0){            return 0;        }    }    return 1;}int main(){    int N;    N = readInt();    int ans; ans = 0;    for(int i=0;i<n;i+=1){        if(isPrime(i)){            ans += i;        }    }    char[20] buffer;    intToStr(ans, buffer);    write(buffer);    return 0;}