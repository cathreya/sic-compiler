int pow(int base, int expo){
    int prod; prod = 1;
    for(int i=0;i<expo;i+=1){
        prod = prod*base;
    }
    return prod;
}

int g(int n, int k){
    int sum; sum = 0;
    for(int i=0;i<n;i+=1){
        sum += pow(i,k);
    }
    return sum;
}

int main(){
    int N; int k;
    read(N);
    read(k);
    int ans = g(N,k);

    print(ans);
}