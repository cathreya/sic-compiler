int isPrime(int n){
    for(int i=2;i*i<=n;i+=1){
        if(n%i==0){
            return 0;
        }
    }
    return 1;
}

int main(){
    int N;
    read(N);
    int ans; ans = 0;
    for(int i=2;i<N;i+=1){
        if(isPrime(i)){
            ans += i;
        }
    }

    print(ans);

    return 0;
}